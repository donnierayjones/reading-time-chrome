{
  "name": "Reading Time",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "Add a reading time indicator to Instapaper and Readability",
  "content_scripts": [
    {
      "matches": [
        "http://www.instapaper.com/*",
        "https://www.instapaper.com/*",
        "http://www.readability.com/*"
      ],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
